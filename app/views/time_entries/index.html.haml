= provide(:title, "Time Logs")
.cell.callout.success
  = render "play_stop_form"
.cell
  .callout.primary
    = render "shared/time_table/table_head", time_entries: @time_entries, controls: true, total: false
  .digg_pagination
    = will_paginate @time_entries
  - if @time_entries.empty?
    .callout.warning
      %p.text-center There is no time entry yet. Your time entries will be listed here.
  
.cell
  = link_to "Enter New Time Manually", new_time_entry_path, class: "button small"